{
  "name": "covid-19",
  "version": "0.1.0",
  "description": "covid-19",
  "engines": {
    "node": "10.14.1",
    "npm": "6.4.1"
  },
  "scripts": {
    "eslint-check": "eslint --print-config .eslintrc.js --ignore-pattern ./.eslintrc.js | eslint-config-prettier-check",
    "lint": "eslint \"**/*.js\" --ignore-pattern ./.eslintrc.js",
    "lint-diff": "git diff --diff-filter=ACM --name-only --cached --relative | grep \\\\.js$ | xargs eslint",
    "lint-fix": "npm run lint -- --fix",
    "prestart": "cp -n .env.example .env.development; npm run lint;",
    "start": "docker-compose up -d",
    "migrate-and-start": "npm run migrations; nodemon server.js;",
    "migrations": "npx sequelize db:migrate",
    "create-migration": "npx sequelize migration:create --config ./migrations/config.js --migrations-path ./migrations/migrations --name",
    "migrations-undo": "npx sequelize db:migrate:undo",
    "migrations-undo-all": "npx sequelize db:migrate:undo:all"
    "test": "jest",
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "server.js",
  "homepage": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ".git"
  },
  "bugs": {
    "url": "/issues"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "pg": "^8.0.2",
    "pino": "^6.1.1",
    "pino-pretty": "^4.0.0",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1",
    "umzug": "^2.3.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-wolox": "^4.0.0",
    "eslint-config-wolox-node": "^3.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.15.3",
    "prettier-eslint": "^9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-diff",
      "pre-push": "npm test"
    }
  }
}